{
	"theme": {
		"mode": "dark",
		"light": "Catppuccin Latte",
		"dark": "Catppuccin Mocha"
	},
	"icon_theme": "Catppuccin Mocha",

	"base_keymap": "Cursor",

	"buffer_font_family": "JetBrainsMonoNL Nerd Font Mono",
	"buffer_font_fallbacks": ["JetBrains Mono NL", "Zed Plex Mono"],
	"buffer_font_features": { "calt": false },
	"buffer_font_size": 12,
	"buffer_font_weight": 400,
	"buffer_line_height": "comfortable",

	"ui_font_family": "JetBrainsMonoNL Nerd Font Mono",
	"ui_font_fallbacks": ["JetBrains Mono NL", "Zed Plex Mono"],
	"ui_font_features": { "calt": false },
	"ui_font_size": 12,
	"ui_font_weight": 400,

	"vim_mode": true,

	"restore_on_startup": "last_session",

	"cursor_blink": false,

	"title_bar": {
		"show_sign_in": false,
		"show_branch_icon": false,
		"show_branch_name": false,
		"show_user_picture": false,
		"show_project_items": false,
		"show_onboarding_banner": false
	},

	"minimap": {
		"show": "always"
	},

	"middle_click_paste": false,

	"relative_line_numbers": true,

	"inlay_hints": {
		"enabled": true
	},

	"outline_panel": {
		"button": false,
		"dock": null
	},
	"collaboration_panel": {
		"button": false,
		"dock": null
	},
	"chat_panel": {
		"button": "never",
		"dock": null
	},
	"git_panel": {
		"dock": "left",
		"button": true
	},
	"notification_panel": {
		"button": false,
		"dock": null
	},
	"agent": {
		"button": false
	},

	"tabs": {
		"git_status": true,
		"show_diagnostics": "all"
	},

	"hard_tabs": true,
	"tab_size": 4,

	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},

	"auto_update": false,

	"diagnostics": {
		"inline": {
			"enabled": true
		}
	},

	"edit_predictions": {
		"mode": "subtle"
	},

	"auto_install_extensions": {},

	"ssh_connections": [],
	"read_ssh_config": true
}
